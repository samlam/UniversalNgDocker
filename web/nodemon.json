{
    "legacyWatch":true,
    "restartable": "rs",
    "ignore": [
        ".git",
        "./node_modules/**/*"
    ],
    "verbose": true,
    "execMap": {
        "js": "node --harmony"
    },
    "execOptions":{
        "args":["--inspect=0.0.0.0:5858"]
    },
    "watch": [
        "./dist/server.js",
        "./dist"
    ],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "html,js,json,css",
    "dump": false
}